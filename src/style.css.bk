  --scrollbar-color: var(--gray-6);
  --note-box-shadow: var(--box-shadow);
  --tools-header-background: var(--gray-3);
  --view-header-background: var(--gray-1);
  --tools-overlay-background: var(--gray-0);
  --tools-button-hover: var(--blue-3);
  --tools-button-active: var(--blue-4);
  --tools-overlay-button-hover: var(--blue-1);
  --tools-overlay-button-active: var(--blue-2);
  --outline-color: var(--blue-5);
  --outline-hover: var(--gray-2);
  --outline-selected: var(--blue-1);
  --color-theme-primary: #00a5e4;
  --color-theme-secondary: #242933;
  --color-font-primary: var(--text-color);
  /* --color-font-primary: #232229; */
  --color-font-secondary: #5b5c68;
  --color-gray-1: var(--component-background);
  /* --color-gray-1: #fff; */
