  justify-content: center;
  height: 28px;
  margin: 0 auto;
}

  .zoom-overlay .Button {
    margin: 0 1px;
    height: 100%
  }

  .zoom-overlay .ActionButton {
    width: 32px;
    height: 32px;
  }

  .zoom-overlay .Button:last-child {
    margin-right: 0
  }

  .zoom-overlay .Button:first-child {
    margin-left: 0
  }

  .zoom-overlay .toggle-element-button {
    height: 100%
  }

  .zoom-overlay .toggle-zoom-overlay {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    min-width: 68px;
    border-radius: 4px;
    position: relative;
    height: 100%;
    color: var(--text-color);
    margin-right: 1px
  }

  .zoom-overlay .toggle-zoom-overlay:hover {
    background-color: var(--view-header-button-hover)
  }

  .zoom-overlay .toggle-zoom-overlay.active {
    background-color: var(--view-header-button-active)
  }

    .zoom-overlay .toggle-zoom-overlay .textarea {
      resize: none;
      border: none;
      outline: none;
      color: inherit;
      background-color: transparent;
      font-weight: 700;
      width: 40px;
      min-width: 28px;
      text-align: right
    }

    .zoom-overlay .toggle-zoom-overlay .Button.active,
    .zoom-overlay .toggle-zoom-overlay .Button:hover {
      background: none!important
    }

    .zoom-overlay .toggle-zoom-overlay .toggle-element-button {
      width: 26px;
      margin-left: 4px;
      margin-top: -2px;
      font-size: 16px;
      height: 26px
    }

    .zoom-overlay .toggle-zoom-overlay .toggle-zoom-overlay-text {
      border-radius: 4px;
      align-items: center;
      margin-right: -8px;
      font-weight: 700;
      height: 100%;
      text-align: center;
      display: flex
    }

@keyframes spin {
  0% {
    transform: rotate(0deg)
  }
  to {
    transform: rotate(1turn)
  }
}