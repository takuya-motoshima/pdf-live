  --view-header-button-active: var(--blue-2);
  --popup-button-hover: var(--blue-1);
  --popup-button-active: var(--blue-2);
  --primary-button: var(--blue-5);
  --primary-button-text: var(--gray-1);
  --primary-button-hover: var(--blue-6);
  --secondary-button-text: var(--blue-5);
  --secondary-button-hover: var(--blue-6);
  --box-shadow: var(--gray-6);
  --document-box-shadow: var(--gray-7);
  --panel-background: var(--gray-1);
  --divider: var(--gray-5);
  --side-panel-border: var(--divider);
  --border: var(--gray-5);
  --focus-border: var(--blue-5);
  --color-palette-border: var(--gray-8);
  --modal-negative-space: rgba(0, 0, 0, 0.3);
  --spinner-negative-space: rgba(0, 0, 0, 0.3);
  --white-color-palette-border: var(--border);
